    <children>
        <Button fx:id="menuButton" layoutX="14.0" layoutY="28.0" onAction="#goMenu" text="Inicio" />
      <Button fx:id="informButton" layoutX="72.0" layoutY="28.0" mnemonicParsing="false" onAction="#goInfo" text="Información" />
      <Button fx:id="whoWeAreButton" layoutX="160.0" layoutY="28.0" mnemonicParsing="false" onAction="#goWhoWeAre" text="¿Quienes somos?" />
      <Button fx:id="DonateButton" layoutX="316.0" layoutY="28.0" mnemonicParsing="false" onAction="#goDonate" text="Donar" />
    </children>
</AnchorPane>
