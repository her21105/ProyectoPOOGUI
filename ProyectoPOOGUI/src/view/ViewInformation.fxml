<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewInformationController">
   <children>
      <Button fx:id="menuButton" layoutX="47.0" layoutY="43.0" mnemonicParsing="false" onAction="#goMenu" text="inicio" />
      <Button fx:id="informButton" layoutX="124.0" layoutY="43.0" mnemonicParsing="false" onAction="#goInfo" text="Información" />
      <Button fx:id="whoWeAreButton" layoutX="248.0" layoutY="43.0" mnemonicParsing="false" onAction="#goWhoWeAre" text="¿Quienes somos?" />
      <Button fx:id="DonateButton" layoutX="382.0" layoutY="43.0" mnemonicParsing="false" onAction="#goDonate" text="Donar" />
      <ComboBox fx:id="AnimalList" layoutX="457.0" layoutY="116.0" onAction="#InfoAnimals" prefWidth="150.0" />
      <ImageView fx:id="imgAnimals" fitHeight="122.0" fitWidth="167.0" layoutX="27.0" layoutY="98.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="ShowInfo" layoutX="213.0" layoutY="103.0" prefHeight="250.0" prefWidth="203.0" />
   </children>
</AnchorPane>
